<%= simple_form_for([contact], remote: true) do |f| %>
<%= f.error_notification %>
<div class="form-inputs">
  <%= f.input :name %>
  <%= f.input :email %>
  <%= f.input :message %>
</div>
<div class="d-flex text-center confirm-form">
  <input type="submit" value="Send Message" class="button">
</div>
<% end %>

